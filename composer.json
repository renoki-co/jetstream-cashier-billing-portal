{
    "name": "renoki-co/jetstream-cashier-billing-portal",
    "description": "Jetstream Cashier Billing Portal is a simple scaffolding billing portal to manage subscriptions, built on top of Jetstream & Cashier Register.",
    "keywords": ["laravel", "php", "cashier", "billing", "portal", "jetstream", "stripe", "saas", "plans", "subscription"],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/jetstream-cashier-billing-portal",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/jetstream": "^2.3",
        "renoki-co/cashier-register": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\BillingPortal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\BillingPortal\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "claudiodekker/inertia-laravel-testing": "^1.2.0",
        "doctrine/dbal": "^2.6|^3.0",
        "laravel/legacy-factories": "^1.1",
        "mockery/mockery": "^1.4",
        "orchestra/database": "^6.0",
        "orchestra/testbench": "^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\BillingPortal\\BillingPortalServiceProvider"
            ]
        }
    }
}
